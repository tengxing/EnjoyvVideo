<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.query">
    <select id="queryInfo" resultType="com.domain.movies" parameterType="java.lang.String">
        select * from teacher where name Like "%"#{name}"%";
